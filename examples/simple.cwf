(mode default
  (set term alacritty)
  (set mod Alt)
  (bind (and (val mod) Enter) (exec (cmd (val term))))
	(bind e (exec (cmd alacritty &)))
	(exec (cmd echo))
)

(exec (cmd ls -lh))

(exec (and 
  (pipe (cmd echo /dev/unrandom) 
        (pipe (cmd head -n 10) (cmd cat))) 
  (cmd echo "that was \"unlikely\"  ")))

(bind (and (val mod) d) (switch default))
